{
  "name": "groverlap",
  "version": "1.0.0",
  "description": "A range overlap detection tool for graphical representation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:dev": "jest --config jestconfig.json --watch --verbose false",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntocampos/groverlap.git"
  },
  "keywords": [
    "range",
    "overlap",
    "graphical",
    "detection"
  ],
  "author": "Mois√©s Neto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntocampos/groverlap/issues"
  },
  "homepage": "https://github.com/ntocampos/groverlap#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {}
}
